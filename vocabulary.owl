                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ] .


###  http://localhost:8090/object#Ammunition
<http://localhost:8090/object#Ammunition> owl:equivalentClass [ rdf:type owl:Class ;
                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :partOf ;
                                                                                owl:someValuesFrom <http://localhost:8090/object#FireWeapon>
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :partOf ;
                                                                                owl:someValuesFrom <http://localhost:8090/object#SpecialWeapon>
                                                                              ]
                                                                            )
                                                              ] .


###  http://localhost:8090/person#Denouncer
<http://localhost:8090/person#Denouncer> owl:equivalentClass <http://localhost:8090/person#Victim> .


###  http://localhost:8090/person#Victim

###  http://localhost:8090/robbery#Robbery
<http://localhost:8090/robbery#Robbery> owl:equivalentClass [ owl:intersectionOf ( <http://localhost:8090/crime#Crime>
                                                                                   [ rdf:type owl:Class ;
                                                                                     owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                     owl:onProperty :hasContactWith ;
                                                                                                     owl:someValuesFrom <http://localhost:8090/person#Criminal>
                                                                                                   ]
                                                                                                   [ rdf:type owl:Restriction ;
                                                                                                     owl:onProperty :hasContactWith ;
                                                                                                     owl:someValuesFrom <http://localhost:8090/person#Victim>
                                                                                                   ]
                                                                                                 )
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :accomplished ;
                                                                                     owl:someValuesFrom <http://localhost:8090/person#Criminal>
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :affects ;
                                                                                     owl:someValuesFrom <http://localhost:8090/person#Victim>
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :happensAt ;
                                                                                     owl:someValuesFrom <http://localhost:8090/local#Local>
                                                                                   ]
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty :own ;
                                                                                     owl:someValuesFrom <http://localhost:8090/object#Object>
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ] .


