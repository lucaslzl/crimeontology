                                                                             owl:onProperty :classify ;
                                                                             owl:someValuesFrom <http://localhost:8090/crime#Crime>
                                                                           ]
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :own ;
                                                                             owl:someValuesFrom <http://localhost:8090/object#FireWeapon>
                                                                           ]
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ] .


###  http://localhost:8090/ec3#EC3
<http://localhost:8090/ec3#EC3> owl:equivalentClass [ owl:intersectionOf ( <http://localhost:8090/ec#EC>
                                                                           [ rdf:type owl:Class ;
                                                                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty :own ;
                                                                                             owl:someValuesFrom <http://localhost:8090/object#Artwork>
                                                                                           ]
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty :own ;
